    "react-date-range": "^0.2.4",
    "react-dom": "^15.2.1",
    "react-dropzone": "^3.5.3",
    "react-form-simple-schema": "0.0.3",
    "react-helmet": "^3.1.0",
    "react-if": "^2.0.5",
    "react-komposer": "^1.3.0",
    "react-material-select": "^1.1.0",
