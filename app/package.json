{
  "name": "Remotiv",
  "version": "0.0.3",
  "description": "Remotive platform",
  "scripts": {
    "test": "mocha .scripts/mocha_boot.js client/**/tests/**/*.js --compilers js:babel-core/register",
    "test-server": "mocha server/**/tests/**/*.js --compilers js:babel-core/register",
    "lint": "eslint ./",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:bosstechlabs/remotiv-platform.git"
  },
  "author": "Jr Reyes <jr.evilboss@gmail.com> (https://github.com/evilboss)",
  "keywords": [
    "node",
    "mongo",
    "express",
    "React",
    "Mantra"
  ],
  "contributors": [
    "Aaron Randrup <aaronrandrup@gmail.com> (https://github.com/aaronra)"
  ],
  "license": "UNLICENSED",
  "homepage": "",
